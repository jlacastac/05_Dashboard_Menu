            android:exported="false" />
        <activity
            android:name=".Activities.GalleryActivity"
            android:theme="@style/GaleriaTheme"
            android:exported="false" />
        <activity
            android:name=".Activities.QuizActivity"
            android:theme="@style/QuizzesTheme"
            android:exported="false" />
        <activity
            android:name=".Activities.ResultActivity"
            android:theme="@style/QuizzesTheme"
            android:exported="false" />
        <activity
            android:name=".Activities.EdadCaninaActivity"
            android:theme="@style/PerreteTheme"
            android:exported="false" />
        <activity
            android:name=".Activities.UsersActivity"
            android:theme="@style/AmigosTheme"
            android:exported="false" />
        <activity
            android:name=".Activities.CalculadoraActivity"
            android:theme="@style/CalculadoraTheme"
            android:exported="false" />
        <activity
            android:name=".Activities.MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>